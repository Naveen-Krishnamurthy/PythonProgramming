birthday={
    "Naveen":"10 Nov",
    "Girish":"21st Feb",
    "Sachin":"24th Apr"}

print("Enter the name :")
name=input()

if name in birthday :
    print("Found your birthday :"+birthday[name])
else :
    print("What's your birthday :")
    newName=input()
    print("Enter your birthday :")
    birthday[newName]=input()
    print("DB updated ":+birthday)