#Method to check the valid phone number without using the regular expression in python

def checkPhoneNumber(phNumber): 
    if len(phNumber)!=12 :
        return False
    
    for i in range(0,3) :
        if not phNumber[i].isdecimal() :
            return False
        
    if phNumber[3]!='-' :
        return False
    
    for i in range(4,7) :
        if not phNumber[i].isdecimal() :
            return False
        
    if phNumber[7]!='-' :
        return False
    
    for i in range(8,12) :
        if not phNumber[i].isdecimal() :
            return False    
    return True

print(checkPhoneNumber("ABCE"))    