print("Addition "+repr(2+2))
print("Sub "+repr(98-44))
print("Floored Division "+repr(22//4))
print("Division "+repr(22/4))
print("Modules "+repr(22%4))
print("Exponent "+repr(3**4))

# Operator precedence
print(2+3*6)
print(repr((2+3)*6))
print(repr(48565878 * 578453))
print(repr(2**8))
print(23/7)
print(23//7)
print(repr(23%7))
print(repr(2 +       2))
val=(5-1)*((7+1)/(3-1))
print(val)
